import java.util.Scanner;

public class main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int n = Integer.parseInt(scan.nextLine());
        int leftNumbers = 0;
        int rightNumbers = 0;

        for (int i = 0; i < n; i++) {
            int numbers1 = Integer.parseInt(scan.nextLine());
            leftNumbers += numbers1;
        }
        for (int i = 0; i < n; i++) {
            int numbers2 = Integer.parseInt(scan.nextLine());
            rightNumbers += numbers2;
        }
        if (leftNumbers == rightNumbers){
            System.out.printf("Yes, sum = %d%n", leftNumbers);
        }else {
            int diff = Math.abs(leftNumbers - rightNumbers);
            System.out.printf("No, diff = %d", diff);
        }
    }
}

