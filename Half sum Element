import java.util.Scanner;

public class main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int n = Integer.parseInt(scan.nextLine());
        int max = Integer.MIN_VALUE;
        int sum = 0;

        for (int i = 1; i <= n; i++) {
            int numbers = Integer.parseInt(scan.nextLine());
            sum += numbers;

            if (numbers > max){
                max = numbers;
            }
        }
        int sumOthers = sum - max;

        if (sumOthers == max){
            System.out.printf("Yes %nSum = %d", max);
        }else {
            System.out.printf("No %nDiff = %d", Math.abs(sumOthers - max));
        }

    }
}
